{
  "name": "@rc-ui/base",
  "type": "module",
  "version": "0.0.1",
  "description": "rc ui base",
  "sideEffects": false,
  "main": "./lib/base.cjs",
  "module": "./lib/base.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/base.js"
      },
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/base.cjs"
      }
    },
    "./style.css": {
      "import": "./lib/style.css",
      "require": "./lib/style.css"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*.js"
      ],
      "./style.css": [
        "./lib/style.css"
      ]
    }
  },
  "files": [
    "lib",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "vite build",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": ">= 17.0.0",
    "react-dom": ">= 17.0.0"
  },
  "keywords": [
    "react"
  ],
  "author": "hunghg255 <giahunghust@gmail.com>",
  "homepage": "https://github.com/hunghg255/rc-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunghg255/rc-ui.git",
    "directory": "packages/transformers"
  },
  "bugs": "https://github.com/hunghg255/rc-ui/issues"
}
